{
  "name": "wechat-starter",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' -r tsconfig-paths/register src/index.ts",
    "build": "NODE_ENV=production rimraf dist && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "prepare": "husky install"
  },
  "keywords": [
    "微信",
    "wechat",
    "sample"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "serve-static": "^1.15.0",
    "wechat": "^2.1.0",
    "wechat-api": "^1.35.1",
    "wechat-oauth": "^1.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/serve-static": "^1.15.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.1",
    "install-deps-postmerge": "^2.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.8.4"
  }
}
